{
  "name": "Wine Butler Bottle Helper",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "img/main.png",
    "default_popup": "popup.html",
    "default_title": "Wine Butler Bottle Helper"
  }
}